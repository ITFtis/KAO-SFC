
<iframe src="@(ViewBag.PageUrl)"></iframe>
<style>
    .body-content {
        width: 100% !important;
        height: 100%;
        padding: 0;
    }
    .body-content > iframe:first-child {
        width: 100%;
        height:calc( 100% -  8px );
        /*min-height: calc(100% - 60px );*/
    }
    body > hr, body > footer{
        display:none;
    }
</style>
<script>
    helper.misc.showBusyIndicator();
    //$('.body-content > iframe:first-child').load(function () {
    //    helper.misc.hideBusyIndicator();
    //});
    $('.body-content > iframe:first-child').on("load",function () {
        helper.misc.hideBusyIndicator();
    });
</script>

